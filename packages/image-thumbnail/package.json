{
    "name": "@react-packages/image-thumbnail",
    "version": "1.1.0",
    "description": "React component to create a thumbnail of an exisiting `<img/>` element without creating a new `<img/>` with the same `src`. Useful for a non-cached/dynamic image because of no double request to the server.",
    "author": "AT Mulyana <atmulyana@yahoo.com>",
    "license": "MIT",
    "repository": "https://github.com/atmulyana/react-packages",
    "keywords": [
        "react",
        "component",
        "image",
        "thumnail",
        "web",
        "html"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "node ../build-index.js image-thumbnail common rect",
        "prepack": "npm run build && shx cp -u ../../LICENSE ./LICENSE"
    },
    "exports": {
        ".": {
            "types": "./index.d.ts",
            "import": "./index.mjs",
            "default": "./index.js"
        }
    },
    "files": [
        "index.*"
    ],
    "dependencies": {
        "@react-packages/common": "^1.0.0",
        "@react-packages/rect": "^1.0.0",
        "javascript-common": ">=0.2.0"
    },
    "devDependencies": {
        "@types/react": "^19.1.13",
        "typescript": "^5.9.2"
    },
    "peerDependencies": {
        "react": ">=16.8.0"
    }
}
